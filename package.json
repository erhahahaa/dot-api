{
  "name": "dot-coaching",
  "version": "1.0.50",
  "description": "Visual coaching platform",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build --target=bun ./src/index.ts --outfile=./dist/server.js",
    "compile": "bun build --target=bun --compile ./src/index.ts --outfile=./dist/server",
    "start": "bun --bun run dist/server.js",
    "db:gen": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.1.1",
    "@elysiajs/cors": "^1.1.0",
    "@elysiajs/jwt": "^1.1.0",
    "@elysiajs/swagger": "^1.1.0",
    "@supabase/supabase-js": "^2.45.0",
    "drizzle-orm": "^0.31.4",
    "drizzle-typebox": "^0.1.1",
    "elysia": "^1.1.5",
    "firebase-admin": "^12.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "midtrans-client": "^1.3.1",
    "midtrans-client-typescript": "github:FerryAr/midtrans-client-typescript",
    "postgres": "^3.4.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.25",
    "bun-types": "latest",
    "drizzle-kit": "^0.22.8"
  },
  "type": "module",
  "module": "src/index.js"
}
